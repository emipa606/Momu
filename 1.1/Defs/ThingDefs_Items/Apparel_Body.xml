<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="ApparelMakeableBase">
    <defName>Apparel_Momu_Poncho</defName>
    <label>Momu Poncho</label>
    <description>A long, cloak-like garment to help you keep cool in the desert. Humans can wear it, too, but only if they have the same body type as the Momu!</description>
    <techLevel>Medieval</techLevel>
    <recipeMaker>
      <recipeUsers>
        <li>ElectricTailoringBench</li>
        <li>HandTailoringBench</li>
      </recipeUsers>
    </recipeMaker>
    <graphicData>
      <texPath>Items/Apparel/Poncho/Poncho</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <statBases>
      <WorkToMake>8000</WorkToMake>
      <Mass>3</Mass>
      <StuffEffectMultiplierArmor>0.60</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.6</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.85</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>3</EquipDelay>
    </statBases>
    <thingCategories>
      <li>Apparel</li>
    </thingCategories>
    <costStuffCount>80</costStuffCount>
    <stuffCategories>
      <li>Leathery</li>
      <li>Fabric</li>
    </stuffCategories>
    <generateCommonality>0.3</generateCommonality>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
      </bodyPartGroups>
      <wornGraphicPath>Items/Apparel/Poncho/Poncho</wornGraphicPath>
      <layers>
        <li>Shell</li>
      </layers>
      <tags>
        <li>Momu_Clothes</li>
      </tags>
    </apparel>
    <comps>
      <li Class="Momu.CompProperties_ApparelBodyRestriction">
        <AllowedBodyTypes>
          <li>Male</li>
          <li>Female</li>
        </AllowedBodyTypes>
      </li>
    </comps>
  </ThingDef>
